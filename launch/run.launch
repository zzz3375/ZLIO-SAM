<launch>

    <arg name="project" default="lio_sam_mid360_pointcloud2"/>
    
    <!-- Parameters -->
    <rosparam file="$(find lio_sam_mid360_pointcloud2)/config/params.yaml" command="load" />

    <!--- LOAM -->
    <include file="$(find lio_sam_mid360_pointcloud2)/launch/include/module_loam.launch" />

    <!--- Robot State TF -->
    <include file="$(find lio_sam_mid360_pointcloud2)/launch/include/module_robot_state_publisher.launch" />

    <!--- Run Navsat -->
    <include file="$(find lio_sam_mid360_pointcloud2)/launch/include/module_navsat.launch" />

    <!--- Run Rviz-->
    <include file="$(find lio_sam_mid360_pointcloud2)/launch/include/module_rviz.launch" />

</launch>
